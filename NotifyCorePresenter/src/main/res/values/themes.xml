<resources xmlns:tools="http://schemas.android.com/tools">   <!-- Definitions -->
  <style name="text_parent">
    <item name="fontFamily">@font/iran_sans</item>
  </style>

  <style name="text_head1" parent="text_parent">
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">@dimen/text_xxxl</item>
  </style>

  <style name="text_head2" parent="text_parent">
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">@dimen/text_xxl</item>
  </style>

  <style name="text_head3" parent="text_parent">
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">@dimen/text_xl</item>
  </style>

  <style name="text_head4" parent="text_parent">
    <item name="android:textSize">@dimen/text_l</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="text_head5" parent="text_parent">
    <item name="android:textSize">@dimen/text_m</item>
  </style>

  <style name="text_head6" parent="text_parent">
    <item name="android:textSize">@dimen/text_s</item>
  </style>   <!-- Base application theme. -->

  <style name="subtitle" parent="text_head6"></style>

  <style name="app_button" parent="text_head5">
    <item name="android:textColor">@color/white</item>
    <item name="android:textAllCaps">false</item>
  </style>

  <style name="DialogTheme.Dark" parent="Theme.AppCompat.Dialog.MinWidth">
    <item name="colorAccent">@color/colorAccent</item>
  </style>

  <attr name="windowBackground" format="reference" />

  <attr name="cardStyle" format="reference" />

  <attr name="dialogTheme" format="reference" />

  <attr name="colorGithub" format="reference" />

  <attr name="thumbnailBackground" format="reference" />

  <attr name="navigationBarColor" format="reference" />

  <attr name="colorExpiring" format="reference" />

</resources>

